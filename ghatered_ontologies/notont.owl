            &lt;/rdf:Description&gt;
        </contact:_example>
        <rdfs:comment>emailAddress is a string.  Use of this is discouraged. Use :mailbox instead </rdfs:comment>
        <rdfs:domain rdf:resource="&contact;SocialEntity"/>
        <rdfs:range rdf:resource="&contact;_EmailAddress"/>
    </owl:ObjectProperty>
    


