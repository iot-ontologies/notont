        </contact:_example>
        <rdfs:range rdf:resource="&contact;Mailbox"/>
        <rdfs:domain rdf:resource="&contact;SocialEntity"/>
        <contact:_addressProperty rdf:resource="&contact;mailboxURI"/>
    </owl:ObjectProperty>
    


