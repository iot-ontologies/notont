        <rdf:type rdf:resource="&owl;InverseFunctionalProperty"/>
        <rdfs:label>email</rdfs:label>
        <contact:_example rdf:datatype="&rdf;XMLLiteral">
            &lt;rdf:Description rdf:about=&quot;http://www.w3.org/2000/10/swap/pim/contact#_Dan&quot;&gt;
